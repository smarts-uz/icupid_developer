function ChangeMenuItem(menuid){
	
	<!-- SWAP MENU ITEMS -->	
	StoreVal1 = document.getElementById('nkey'+menuid).value;
	StoreVal2 = document.getElementById('name'+menuid).value;
	
	LoID = menuid+1;
	StoreVal3 = document.getElementById('nkey'+LoID).value;
	StoreVal4 = document.getElementById('name'+LoID).value;
	
	document.getElementById('nkey'+menuid).value = StoreVal3;
	document.getElementById('name'+menuid).value = StoreVal4;
	document.getElementById('nkey'+LoID).value = StoreVal1;
	document.getElementById('name'+LoID).value = StoreVal2;
}
</script>



