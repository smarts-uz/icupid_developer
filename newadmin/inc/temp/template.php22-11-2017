	$isCSS = substr($_REQUEST['file'], -3, 3);
	$Server_Path = FilterPath();
	$DontNeedPath =0;

	if(strpos($_POST['file'], "&dd=1") != false) {	
			
		$DontNeedPath =1;
		$nFile = explode("&dd=1",$_POST['file']);
		$_POST['file']=$nFile[0];
		$path =$Server_Path.$nFile[0]; 		  

  	}elseif(isset($_POST['file']) && $_POST['file'] == "index.php"){		  
	
	  	$path =$Server_Path."/inc/templates/".D_TEMP."/".$_POST['file'];  

	}elseif(isset($_POST['file']) && $_POST['file'] == "template.css"){		  
		
		$path =$Server_Path."/inc/templates/".D_TEMP."/".$_POST['file'];  

	}elseif(isset($_POST['file']) && $_POST['file'] == "header.php" || $_POST['file'] == "footer.php" || $_POST['file'] == "menu.php" ){

		$path =FilterPath()."/inc/templates/".D_TEMP."/".$_POST['file'];	

	}elseif(isset($_POST['file']) && $isCSS =="css"){		  
	
		  	$path =$Server_Path."/".$_POST['file'];  

  	}elseif(isset($_POST['file'])){		  	
	
		$pArray = explode("**",$_POST['file']);	

		if(!isset($pArray[1])){
			if(strpos($_POST['file'] , 'layout')){
				$path =$Server_Path."/inc/templates".$_POST['file'];		
			}
			else{
				$path =$Server_Path."/inc/templates/".D_TEMP."/".$_POST['file'];		
			}
		}else{
			//database content
			$DBFile=1;
			$path =$pArray[0]."**".$pArray[1];
		}
  	}else{
	
		$path =FilterPath()."/inc/templates/".D_TEMP."/header.php";	
		
	}
		 
	$FTPPATH = str_replace($Server_Path, "", $path);

	$SourceFile = $path;
	$path = str_replace("//","/",$path);
	if(!isset($DBFile)){
	
		if (!is_writable($SourceFile)) {
			@chmod($SourceFile,0755);
		}
		if (is_writable($SourceFile)) {} else {print '<input class="lbOn" id="lbOnAuto" type="hidden" value="pop_chmod.php?path='.$SourceFile.'">';} 
